{
  "name": "apex_tracker",
  "version": "1.0.0",
  "private": true,
  "main": "dist/app.js",
  "author": "md akib <mohiuddinmostafakamal@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node dist/app.js",
    "server": "nodemon src/app.ts",
    "build:server": "tsc -p .",
    "client": "yarn --cwd ./client serve",
    "build:client": "yarn --cwd ./client build",
    "dev": "concurrently \" yarn server \" \" yarn client \"",
    "build": "yarn build:server & yarn build:client"
  },
  "engines": {
    "node": "12.7.0",
    "npm": "6.10.0"
  },
  "keywords": [
    "apex legend",
    "express",
    "typescript",
    "nodejs"
  ],
  "description": "app to track apex legend stats for an user with provided credentials",
  "dependencies": {
    "axios": "^0.19.0",
    "compression": "^1.7.4",
    "concurrently": "^4.1.1",
    "config": "^3.2.2",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "helmet": "^3.20.0",
    "inversify": "^5.0.1",
    "module-alias": "^2.2.1",
    "morgan": "^1.9.1",
    "reflect-metadata": "^0.1.13",
    "core-js": "^2.6.5",
    "register-service-worker": "^1.6.2",
    "vue": "^2.6.10",
    "vue-class-component": "^7.0.2",
    "vue-property-decorator": "^8.1.0",
    "vue-router": "^3.0.3",
    "vuetify": "^2.0.0",
    "vuex": "^3.0.1"
  },
