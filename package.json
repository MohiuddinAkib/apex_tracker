    "@types/debug": "^4.1.5",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/helmet": "^0.0.43",
    "@types/morgan": "^1.7.36",
    "@types/node": "^12.7.1",
    "dotenv": "^8.0.0",
    "nodemon": "^1.19.1",
    "ts-node": "^8.3.0",
    "tsconfig-paths": "^3.8.0",
    "typescript": "^3.5.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.21",
