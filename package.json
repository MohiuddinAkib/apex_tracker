{
  "name": "apex_tracker",
  "version": "1.0.0",
  "private": true,
  "main": "app.js",
  "author": "md akib <mohiuddinmostafakamal@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node dist/app.js",
    "server": "nodemon src/app.ts",
    "build:server": "tsc -p .",
    "client": "yarn --cwd ./client serve",
    "client:install": "yarn --cwd ./client install",
    "build:client": "yarn --cwd ./client build",
    "dev": "concurrently \" yarn server \" \" yarn client \"",
    "build": "yarn build:server && yarn build:client",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false yarn client:install && yarn build:server && yarn build:client"
  },
  "engines": {
    "node": "12.7.0",
    "npm": "6.10.0"
  },
  "keywords": [
    "apex legend",
    "express",
    "typescript",
    "nodejs"
  ],
  "description": "app to track apex legend stats for an user with provided credentials",
  "dependencies": {
    "axios": "^0.21.1",
    "compression": "^1.7.4",
    "concurrently": "^4.1.1",
    "config": "^3.2.2",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "helmet": "^3.20.0",
    "inversify": "^5.0.1",
    "module-alias": "^2.2.1",
    "morgan": "^1.9.1",
    "reflect-metadata": "^0.1.13",
    "@types/axios": "^0.14.0",
    "@types/compression": "^1.0.0",
    "@types/config": "^0.0.34",
    "@types/cors": "^2.8.5",
    "@types/debug": "^4.1.5",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/helmet": "^0.0.43",
    "@types/morgan": "^1.7.36",
    "@types/node": "^12.7.1",
    "dotenv": "^8.0.0",
    "nodemon": "^1.19.1",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  },
  "_moduleAliases": {
    "@": "./dist"
  }
}
